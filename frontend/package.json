		"lucide-react": "^0.544.0",
		"motion": "^12.23.22",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-hot-toast": "^2.6.0",
		"react-redux": "^9.2.0",
		"react-router-dom": "^7.9.1",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.13",
		"zod": "^4.1.11"
