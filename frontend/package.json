		"prettier": "^3.6.2",
		"rollup-plugin-visualizer": "^6.0.3",
		"tw-animate-css": "^1.3.8",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.43.0",
		"vite": "^7.1.6"
	}
